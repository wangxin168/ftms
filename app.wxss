/**app.wxss**/
.header {
  width: 100%;
  height: 129rpx;
  background: white;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  /* align-items: center; */
  justify-content: space-between;
  line-height: 176rpx;
  z-index: 10;
}

.header>view {
  width: 30%;
  text-align: center;
}

.left_top {
  height: 100%;
  line-height: 176rpx;
}

.zong {
  display: flex;
  align-items: center;
  margin-left: 39rpx;

}

.left_top text {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(229, 0, 32, 1);
  height: 100%;
}

.left_top image {
  width: 13rpx;
  height: 8rpx;
  margin-left: 12rpx;
}

.title {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(31, 31, 31, 1);
}
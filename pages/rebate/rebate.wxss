/* pages/rebate/rebate.wxss */
page {
  background: #F7F7F7;
}

.news_list {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 5;
  position: fixed;
  top: 0;
  left: 0;
}

.san_wh {
  width: 27rpx;
  height: 17rpx;
  position: absolute;
  top: 135rpx;
  left: 90rpx;
}

.list_con {
  width: 252rpx;
  top: 149rpx;
  left: 60rpx;
  position: absolute;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 4rpx 0px rgba(148, 148, 148, 0.5);
  border-radius: 9rpx;
  padding: 0 27rpx;
  box-sizing: border-box;
}

.list_con>view {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(43, 43, 43, 1);
  border-bottom: 1px solid #E3E3E3;
  text-align: center;
}

.list_con>view:last-child {
  border: none;
}

.rebate_list {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  /* margin-top: 191rpx; */
}

.rebate_li {
  width: 100%;
  height: 161rpx;
  margin-bottom: 30rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  padding: 38rpx 22rpx 38rpx 46rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.rebate_li image {
  width: 26rpx;
  height: 36rpx;
}

.left_li view:nth-of-type(1) {
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(43, 43, 43, 1);
}

.left_li view:nth-of-type(2) {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(180, 180, 180, 1);
}

.shouquan {
  margin-top: 500rpx;
  padding: 0 165rpx;
  box-sizing: border-box;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(126, 126, 126, 1);
}

.shouquan_btn {
  max-width: 233rpx;
  height: 76rpx;
  background: rgba(229, 0, 32, 1);
  box-shadow: 0px 2rpx 4rpx 0px rgba(255, 158, 171, 1);
  border-radius: 4rpx;
  padding: 0;
  margin-top: 56rpx;
  text-align: center;
  line-height: 76rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  border: none;
  outline: none;
}

.login_box {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.6);
  /* display: none; */
}

.login {
  width: 614rpx;
  height: 535rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 68rpx;
  margin: auto;

}

.index_bott {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
}

.login_con {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  padding: 39rpx 49rpx 57rpx 52rpx;
  box-sizing: border-box;
}

.denglu {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(36, 36, 36, 1);
  margin-bottom: 35rpx;
}

.denglu image {
  width: 26rpx;
  height: 26rpx;
  display: block;

}

.input_text {
  width: 513rpx;
  height: 70rpx;
  border-radius: 4rpx;
  border: 1px solid rgba(221, 221, 221, 1);
  padding: 0 24rpx;
  box-sizing: border-box;
  margin-bottom: 70rpx;
  outline: none;
}

.button_text {
  width: 513rpx;
  height: 70rpx;
  background: rgba(229, 0, 32, 1);
  border-radius: 4rpx;
  border: 1px solid rgba(221, 221, 221, 1);
  text-align: center;
  line-height: 70rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  margin-top: 46rpx;
  outline: none;
  border: none;
}

input::-webkit-input-placeholder {
  color: #B4B4B4;
  font-size: 28rpx;
}
input::-moz-input-placeholder {
  color: #B4B4B4;
  font-size: 28rpx;
}
input::-ms-input-placeholder {
  color: #B4B4B4;
  font-size: 28rpx;
}
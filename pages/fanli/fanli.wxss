/* pages/fanli/fanli.wxss */
page {
  background: #f5f5f5;
}

.fanli_ul {
  width: 100%;
  background: white;
  padding: 12rpx 32rpx 0 32rpx;
  box-sizing: border-box;
}

.fanli_li {
  width: 100%;
  display: flex;
  justify-content: space-between;
  height: 119rpx;
  line-height: 119rpx;
  border-bottom: 1px solid #EAEAEA;
}

.fanli_li text:nth-of-type(1) {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(43, 43, 43, 1);

}

.fanli_li text:nth-of-type(2) {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(229, 0, 32, 1);
}